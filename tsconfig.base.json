{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "strictNullChecks": true,
    "esModuleInterop": true,
    "paths": {
      "@tf2-automatic/bot-data": ["libs/bot-data/src/index.ts"],
      "@tf2-automatic/bot-manager-data": ["libs/bot-manager-data/src/index.ts"],
      "@tf2-automatic/bptf-manager-data": [
        "libs/bptf-manager-data/src/index.ts"
      ],
      "@tf2-automatic/config": ["libs/config/src/index.ts"],
      "@tf2-automatic/dto": ["libs/dto/src/index.ts"],
      "@tf2-automatic/executors": ["libs/executors/src/index.ts"],
      "@tf2-automatic/is-refined-validator": [
        "libs/is-refined-validator/src/index.ts"
      ],
      "@tf2-automatic/is-steamid-validator": [
        "libs/is-steamid-validator/src/index.ts"
      ],
      "@tf2-automatic/item-service-data": [
        "libs/item-service-data/src/index.ts"
      ],
      "@tf2-automatic/locking": ["libs/locking/src/index.ts"],
      "@tf2-automatic/nestjs-events": ["libs/nestjs-events/src/index.ts"],
      "@tf2-automatic/nestjs-steamid-pipe": [
        "libs/nestjs-steamid-pipe/src/index.ts"
      ],
      "@tf2-automatic/nestjs-storage": ["libs/nestjs-storage/src/index.ts"],
      "@tf2-automatic/swagger": ["libs/swagger/src/index.ts"],
      "@tf2-automatic/testing": ["libs/testing/src/index.ts"],
      "@tf2-automatic/tf2-format": ["libs/tf2-format/src/index.ts"],
      "@tf2-automatic/transactional-outbox": [
        "libs/transactional-outbox/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
