{
  "name": "executors",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/executors/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/executors/**/*.ts"
        ]
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/executors",
        "main": "libs/executors/src/index.ts",
        "tsConfig": "libs/executors/tsconfig.lib.json",
        "assets": [
          "libs/executors/*.md",
          {
            "input": "./libs/executors/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/executors/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/executors",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/executors",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/executors/jest.config.ts"
      }
    }
  }
}
